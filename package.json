{
  "name": "connectd-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "db:seed": "ts-node -r tsconfig-paths/register ./src/resources/seed.ts seed",
    "db:clear": "ts-node -r tsconfig-paths/register ./src/resources/seed.ts clear",
    "db:refresh": "ts-node -r tsconfig-paths/register ./src/resources/seed.ts refresh",
    "db:oneTime": "ts-node -r tsconfig-paths/register ./src/resources/seed.ts oneTime",
    "start": "tsc && node -r ./tsconfig-paths-bootstrap.js ./build/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "stage": "npm run start",
    "prod": "npm run start"
  },
  "dependencies": {
    "aws-sdk": "^2.970.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "i18next": "^20.3.4",
    "json5": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.3",
    "socket.io": "^4.1.3",
    "tsconfig-paths": "^3.10.1",
    "twilio": "^3.66.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.6",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.7",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.4.1",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^7.4.7",
    "faker": "^5.5.3",
    "nodemailer": "^6.6.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
